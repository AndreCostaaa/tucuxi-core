                                <analyteId>analyte</analyteId>
                                <unit>mg/l</unit>
                                <molarMass>
                                    <value>10.0</value>
                                    <unit>mol/l</unit>
                                </molarMass>
                                <description/>
                                <errorModel>
                                    <errorModelType>proportional</errorModelType>
                                    <sigmas>
                                        <sigma>
                                            <standardValue>0.1</standardValue>
                                        </sigma>
                                    </sigmas>
                                    <comments/>
                                </errorModel>
                                <comments/>
                            </analyte>
                        </analytes>
                        <dispositionParameters>
                            <parameters>
                                <parameter>
                                    <parameterId>TestA</parameterId>
                                    <unit>kg</unit>
                                    <parameterValue>
                                        <standardValue>1</standardValue>
                                        <aprioriComputation>
                                            <softFormula>
                                                <inputs>
                                                    <input>
                                                        <id>covA</id>
                                                        <type>double</type>
                                                    </input>
                                                </inputs>
                                                <code>
                                                    <![CDATA[return covA;]]>
                                                </code>
                                            </softFormula>
                                            <comments/>
                                        </aprioriComputation>
                                    </parameterValue>
                                    <bsv>
                                        <bsvType>exponential</bsvType>
                                        <stdDevs>
                                            <stdDev>0.2</stdDev>
                                        </stdDevs>
                                    </bsv>
                                    <comments/>
                                </parameter>
                                <parameter>
                                    <parameterId>TestM</parameterId>
                                    <unit>cm</unit>
                                    <parameterValue>
                                        <standardValue>1</standardValue>
                                        <aprioriComputation>
                                            <softFormula>
                                                <inputs>
                                                    <input>
                                                        <id>covM</id>
                                                        <type>double</type>
                                                    </input>
                                                </inputs>
                                                <code>
                                                    <![CDATA[return covM;]]>
                                                </code>
                                            </softFormula>
                                            <comments/>
                                        </aprioriComputation>
                                    </parameterValue>
                                    <bsv>
                                        <bsvType>none</bsvType>
                                    </bsv>
                                    <comments/>
                                </parameter>
                                <parameter>
                                    <parameterId>TestR</parameterId>
                                    <unit>mg/l</unit>
                                    <parameterValue>
                                        <standardValue>1</standardValue>
                                        <aprioriComputation>
                                            <softFormula>
                                                <inputs>
                                                    <input>
                                                        <id>covR</id>
                                                        <type>double</type>
                                                    </input>
                                                </inputs>
                                                <code>
                                                    <![CDATA[return covR;]]>
                                                </code>
                                            </softFormula>
                                            <comments/>
                                        </aprioriComputation>
                                    </parameterValue>
                                    <bsv>
                                        <bsvType>none</bsvType>
                                    </bsv>
                                    <comments/>
                                </parameter>
                                <parameter>
                                    <parameterId>TestS</parameterId>
                                    <unit>h</unit>
                                    <parameterValue>
                                        <standardValue>1</standardValue>
                                        <aprioriComputation>
                                            <softFormula>
                                                <inputs>
                                                    <input>
                                                        <id>covS</id>
                                                        <type>double</type>
                                                    </input>
                                                </inputs>
                                                <code>
                                                    <![CDATA[return covS;]]>
                                                </code>
                                            </softFormula>
                                            <comments/>
                                        </aprioriComputation>
                                    </parameterValue>
                                    <bsv>
                                        <bsvType>none</bsvType>
                                    </bsv>
                                    <comments/>
                                </parameter>
                            </parameters>
                            <correlations/>
                        </dispositionParameters>
                    </analyteGroup>
                </analyteGroups>
                <formulationAndRoutes default='id0'>
                    <formulationAndRoute>
                        <formulationAndRouteId>id0</formulationAndRouteId>
                        <formulation>parenteral solution</formulation>
                        <administrationName>Name</administrationName>
                        <administrationRoute>oral</administrationRoute>
                        <absorptionModel>extra</absorptionModel>
