        <domain>
            <description/>
            <constraints/>
        </domain>
        <covariates>
            <covariate>
                <covariateId>covA</covariateId>
                <covariateName>
					<name lang='en'>Total Body Weight</name>
					<name lang='fr'>Poids total</name>
				</covariateName>
                <description/>
                <unit>kg</unit>
                <covariateType>standard</covariateType>
                <dataType>double</dataType>
                <interpolationType>linear</interpolationType>
                <refreshPeriod>
                    <unit>d</unit>
                    <value>30</value>
                </refreshPeriod>
                <covariateValue>
                    <standardValue>1</standardValue>
                </covariateValue>
                <validation>
                    <errorMessage/>
                    <operation>
                        <softFormula>
                            <inputs>
                                <input>
                                    <id>covA</id>
                                    <type>double</type>
                                </input>
                            </inputs>
                            <code>
                                <![CDATA[return ((covA>= 50) && (covA<= 150));]]>
                                </code>
                            </softFormula>
                            <comments/>
                        </operation>
                        <comments/>
                    </validation>
                    <comments/>
                </covariate>
                <covariate>
                    <covariateId>covM</covariateId>
                    <covariateName>
						<name lang='en'>Body height</name>
						<name lang='fr'>Taille (hauteur)</name>
					</covariateName>
                    <description/>
                    <unit>cm</unit>
                    <covariateType>standard</covariateType>
                    <dataType>double</dataType>
                    <interpolationType>linear</interpolationType>
                    <refreshPeriod>
                        <unit>d</unit>
                        <value>30</value>
                    </refreshPeriod>
                    <covariateValue>
                        <standardValue>1</standardValue>
                    </covariateValue>
                    <validation>
                        <errorMessage/>
                        <operation>
                            <softFormula>
                                <inputs>
                                    <input>
                                        <id>covM</id>
                                        <type>double</type>
                                    </input>
                                </inputs>
                                <code>
                                    <![CDATA[return ((covM > 100) && (covM < 200));]]>
                                    </code>
                                </softFormula>
                                <comments/>
                            </operation>
                            <comments/>
                        </validation>
                        <comments/>
                    </covariate>
                    <covariate>
                        <covariateId>covR</covariateId>
                        <covariateName>
							<name lang='en'>Concentration</name>
							<name lang='fr'>Concentration</name>
						</covariateName>
                        <description/>
                        <unit>mg/l</unit>
                        <covariateType>standard</covariateType>
                        <dataType>double</dataType>
                        <interpolationType>linear</interpolationType>
                        <refreshPeriod>
                            <unit>d</unit>
                            <value>30</value>
                        </refreshPeriod>
                        <covariateValue>
                            <standardValue>1</standardValue>
                        </covariateValue>
                        <validation>
                            <errorMessage/>
                            <operation>
                                <softFormula>
                                    <inputs>
                                        <input>
                                            <id>covR</id>
                                            <type>double</type>
                                        </input>
                                    </inputs>
                                    <code>
                                        <![CDATA[return (true);]]>
                                    </code>
                                </softFormula>
                                <comments/>
                            </operation>
                            <comments/>
                        </validation>
                        <comments/>
                    </covariate>
                    <covariate>
                        <covariateId>covS</covariateId>
                        <covariateName>
							<name lang='en'>Time</name>
							<name lang='fr'>Temps</name>
						</covariateName>
                        <description/>
                        <unit>h</unit>
                        <covariateType>standard</covariateType>
                        <dataType>double</dataType>
                        <interpolationType>linear</interpolationType>
                        <refreshPeriod>
                            <unit>d</unit>
                            <value>30</value>
                        </refreshPeriod>
                        <covariateValue>
                            <standardValue>1</standardValue>
                        </covariateValue>
                        <validation>
                            <errorMessage/>
                            <operation>
                                <softFormula>
                                    <inputs>
                                        <input>
                                            <id>covS</id>
                                            <type>double</type>
                                        </input>
                                    </inputs>
                                    <code>
                                        <![CDATA[return (true);]]>
                                    </code>
                                </softFormula>
                                <comments/>
                            </operation>
                            <comments/>
                        </validation>
                        <comments/>
                    </covariate>
                </covariates>
                <activeMoieties>
                    <activeMoiety>
                        <activeMoietyId>drugtest</activeMoietyId>
                        <activeMoietyName>
